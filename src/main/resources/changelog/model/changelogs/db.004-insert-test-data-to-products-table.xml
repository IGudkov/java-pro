<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns:pro="http://www.liquibase.org/xml/ns/pro" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        changelog/dbchangelog-4.27.xsd">

    <changeSet id="004" author="igudkov">

        <preConditions onFail="MARK_RAN">
                <tableExists tableName="products"/>
        </preConditions>

        <sqlFile path="changelog/model/sql/004-insert-test-data-to-products-table.sql"
                 splitStatements="false"
                 stripComments="false"
                 relativeToChangelogFile="false"
                 encoding="utf-8"
                 dbms="postgresql"/>

        <rollback>
            <sql>
                delete from products;
            </sql>
        </rollback>

    </changeSet>

</databaseChangeLog>